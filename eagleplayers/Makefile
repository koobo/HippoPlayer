# Experimental makefile

COMPRESS=shrinkler -w -p -d -9

SOURCES=\
	bin/activision\ pro \
	bin/aprosys \
	bin/chiptracker \
	bin/david\ whittaker \
	bin/eaglestone \
	bin/earache \
	bin/face\ the\ music \
