	shr/jason\ page.shr \
	shr/jeroen\ tel.shr \
	shr/jochen\ hippel\ 7v.shr \
	shr/jochen\ hippel\ st.shr \
	shr/kris\ hatlelid.shr \
	shr/mark\ cooksey.shr \
	shr/maxtrax.shr \
	shr/mugician\ ii.shr \
	shr/mugician.amp.shr \
	shr/musicmaker4.shr \
	shr/musicmaker8.shr \
	shr/quartet.shr \
	shr/quartet\ st.shr \
	shr/richard\ joseph.shr \
	shr/richard\ joseph\ player.shr \
