	bin/rob\ hubbard \
	bin/rob\ hubbard\ 2 \
	bin/sonix\ music\ driver \
	bin/soundcontrol \
	bin/special\ fx \
	bin/steve\ turner \
	bin/synth\ 4.0 \
	bin/synth\ pack \
	bin/syntracker \
	bin/tcb\ tracker \
	bin/tim\ follin\ ii \
	bin/tme \
	bin/wally\ beben \
	bin/sound\ master \
	bin/soprol \
	bin/delimidi

COMPRESSED_TARGETS=\
	shr/activision\ pro.shr \
	shr/aprosys.shr \
	shr/chiptracker.shr \
	shr/david\ whittaker.shr \
	shr/eaglestone.shr \
	shr/earache.shr \
